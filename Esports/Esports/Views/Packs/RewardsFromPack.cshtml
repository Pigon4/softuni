@model OpenedPackViewModel

@inject IImageConverter _imageConverter
@{
    ViewBag.Title = "Rewards";
}


<body>
    <div class="container" id="box">
        <h1>Here are your rewards!</h1>
        <ul>
            @foreach (var item in Model.Players)
            {
                <li style="display:inline"><img src="@_imageConverter.ConvertToImage(item.Image)"></li>
            }

        </ul>
    </div>
    

</body>

